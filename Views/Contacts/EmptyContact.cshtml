@model IEnumerable<ContactPro.Models.Contact>

@using ContactPro.Services.Interfaces
@using ContactPro.Services
@using ContactPro.Models

@inject IImageService ImageService

<div class="row d-flex justify-content-center mt-5">
    <div class="p-2 col-6">
        <div class="d-flex justify-content-center">

            <h2>No contacts found that match your search string.</h2>
        </div>
        <div class="d-flex justify-content-center">
            <a class="btn btn-outline-primary fw-bold rounded-pill" asp-controller="Contacts" asp-action="Index">Return to Contacts</a>
        </div>
    </div>
</div>